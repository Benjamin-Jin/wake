# exceptions
def test  x = prim "test"
def catch x = prim "catch"
global def try f x = if test x then f (catch x) else x
global def tryelse f g x = if test x then f (catch x) else g x
global def raise x = prim "raise"
global def cast x = prim "cast"

global def reraise f x = if test x then cast x else f x
